<div class="brand-area">
    <div class="container">
        <div class="secondary-title text-center">
            <h6 class="text-uppercase">{{page.header.sec_two.subtitle}}</h6>
            <h3>{{page.header.sec_two.title}}</h3>
        </div>

        <div class="brand-flex-wrapper d-flex flex-wrap justify-content-center">
            {% for brandLogo in page.header.sec_two.images %}
            {% set link = page.find('/images').media[brandLogo].url | e %}
            <div class="brand-item d-inline-flex align-items-center justify-content-center">
                <img src="{{link}}" alt="" class="img-fluid">
            </div>
            {% endfor %}
        </div>
    </div>
</div>